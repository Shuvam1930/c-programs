#include<stdio.h>
#include"arith.h"
int main()
{
	printf("Addition of two number is:%d\n",add(20,10));
	printf("Substraction of two number is:%d\n",sub(20,10));
        printf("multiplication of two number is:%d\n",mul(10,20));
        printf("Division of two number is:%d\n",div(20,10));
        printf("Modulus of two number is:%d\n",mod(20,10));
	return 0;
}

